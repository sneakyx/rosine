<form method="post" action="{{ path('rosine_notes_delete', {'noteId': rosine_note.noteId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ rosine_note.noteId) }}">
    <button class="btn">Delete</button>
</form>
